<?xml version="1.0" encoding="utf-8" ?>
<c2eventsheet>
    <!--All the 'name' attributes are ignored by Construct 2 - they are there for readability only.-->
    <name>Creditos</name>
    <events>
        <event-block>
            <conditions>
                <condition id="-2" name="On start of layout" type="System" />
            </conditions>
            <actions>
                <action id="0" name="Set text" type="Text">
                    <param id="0" name="Text">&quot;Coordenação:&#x0D;&#x0A;Joice Otsuka&#x0D;&#x0A;Delano Medeiros Beder&#x0D;&#x0A;Marcia Rozenfeld&#x0D;&#x0A;&#x0D;&#x0A;Supervisão de equipe:&#x0D;&#x0A;Ivan Rocca&#x0D;&#x0A;&#x0D;&#x0A;Roteiro:&#x0D;&#x0A;Jheniffer Camila Pedro&#x0D;&#x0A;Marcos Vinícius Costa&#x0D;&#x0A;&#x0D;&#x0A;Alunos envolvidos:&#x0D;&#x0A;Daniel Santiago&#x0D;&#x0A;Jessica Sayuri Nishiguchi&#x0D;&#x0A;Felipe Berlini&#x0D;&#x0A;Guilherme H. R. Perego&#x0D;&#x0A;Lucas Oliveira David&#x0D;&#x0A;Catarine Santana Ohnuma&#x0D;&#x0A;Jennifer Lu&#x0D;&#x0A;Jheniffer Camila Pedro&#x0D;&#x0A;Marcos Vinicius Costa&quot;</param>
                </action>
            </actions>
        </event-block>
        <event-block any="1">
            <conditions>
                <condition id="3" name="On touched object" type="Touch">
                    <param id="0" name="Object">botVoltar</param>
                </condition>
                <condition id="6" name="On object clicked" type="Mouse">
                    <param id="0" name="Mouse button">0</param>
                    <param id="1" name="Click type">0</param>
                    <param id="2" name="Object clicked">botVoltar</param>
                </condition>
            </conditions>
            <actions>
                <action id="-25" name="Go to layout (by name)" type="System">
                    <param id="0" name="Layout">&quot;MenuInicial&quot;</param>
                </action>
            </actions>
        </event-block>
        <event-block any="1">
            <conditions>
                <condition id="7" name="Cursor is over object" type="Mouse">
                    <param id="0" name="Object">botVoltar</param>
                </condition>
                <condition id="4" name="Is touching object" type="Touch">
                    <param id="0" name="Object">botVoltar</param>
                </condition>
            </conditions>
            <actions>
                <action id="4" name="Set animation" type="botVoltar">
                    <param id="0" name="Animation">&quot;Selected&quot;</param>
                    <param id="1" name="From">1</param>
                </action>
            </actions>
        </event-block>
        <event-block>
            <conditions>
                <condition id="-22" name="Else" type="System" />
            </conditions>
            <actions>
                <action id="4" name="Set animation" type="botVoltar">
                    <param id="0" name="Animation">&quot;Default&quot;</param>
                    <param id="1" name="From">1</param>
                </action>
            </actions>
        </event-block>
    </events>
</c2eventsheet>
